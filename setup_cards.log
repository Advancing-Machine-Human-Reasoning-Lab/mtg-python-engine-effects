add_play_func_no_target("Nourish", lambda self: [self.controller.gain_life(6)])

add_play_func_no_target("Whitesun's Passage", lambda self: [self.controller.gain_life(5)])

add_play_func_no_target("Moorland Inquisitor", lambda self: [self.card.add_effect('gainAbility', 'First Strike', self, self.game.eot_time)])

add_targets("Reito Lantern", [lambda self, p: p.zone_type == 'GRAVEYARD'], prompts=[])
add_play_func_with_targets("Reito Lantern", lambda self, targets, is_legal_target: [targets[0].change_zone(p.owner.library, -1, False)])

add_targets("Karplusan Wolverine", ['creature or player'], prompts=[])
add_play_func_with_targets("Karplusan Wolverine", lambda self, targets, is_legal_target: [targets[0].take_damage(self, 1)])

add_targets("Elder Druid", ['creature or land or artifact'], prompts=[])
add_play_func_with_targets("Elder Druid", lambda self, targets, is_legal_target: [[targets[0].untap() if targets[0].status.tapped else targets[0].tap()]])

add_play_func_no_target("Songs of the Damned", lambda self: [[self.mana.add_str('B') for i in self.controller.graveyard if i.is_creature]])

